            "rich-text#aldo-crew-banner-header-text-surprise",
            "rich-text#aldo-crew-banner-header-link-benefits"
        ]
    },
    "image#aldo-crew-banner-header-logo": {
        "props": {
            "src": "https://aldoecuador.vteximg.com.br/arquivos/aldocrew-logo.png",
            "blockClass": "aldo-crew-banner-header-logo"
