{
  "search-not-found-layout": {
    "children": [
      "flex-layout.row#notfound"
    ]
  },
  "flex-layout.row#notfound": {
    "children": [
      "flex-layout.col#notfound-left"
    ],
    "props": {
      "blockClass": "wrapperNotFound",
      "fullWidth": true
    }
  },
  "flex-layout.col#notfound-left": {
    "children": [
      "flex-layout.row#searchtitleNotFound",
      "rich-text#contentNotFoundMain",
      "rich-text#introNotFoundMain",
      "flex-layout.row#wrapperLinkNF",
      "back-to-top-button"
    ],
    "props": {
      "verticalAlign": "center",
      "preventVerticalStretch": true,
      "fullWidth": true
    }
  },
  "flex-layout.row#wrapperLinkNF": {
    "children": [
      "flex-layout.col#wrapperLinkNFLeft",
      "flex-layout.col#wrapperLinkNFRight"
    ]
  },
