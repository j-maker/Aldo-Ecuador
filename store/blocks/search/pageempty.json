    "children": [
      "rich-text#contentNotFoundHelp",
      "rich-text#questionNotFoundSearchSales",
      "rich-text#introNotFoundSearchSales"
    ]
  },
  "flex-layout.col#wrapperLinkNFRight": {
    "children": [
      "rich-text#questionNotFoundArticle",
      "rich-text#introNotFoundArticle",
      "rich-text#contentNotFoundArticle",
      "rich-text#questionNotFoundHelp"
    ]
  },
  "flex-layout.row#searchtitleNotFound": {
    "children": [
      "search-title.v2"
    ],
    "props": {
      "blockClass": "NotFoundTitle"
