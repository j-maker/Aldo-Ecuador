    "children": [
      "tab-layout#products-details-tabs"
    ],
    "props": {
      "blockClass": ""
    }
  },
  "tab-layout#products-details-tabs": {
    "children": [
      "flex-layout.row#products-details",
      "tab-content#products-details"
    ],
    "props": {
      "blockClass": "",
      "defaultActiveTabId": "home2"
    }
  },
