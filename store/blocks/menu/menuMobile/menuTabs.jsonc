    "props": {
      "tabId": "home1",
      "blockClass": "wrapperImageSize"
    }
  },
  "tab-content.item#products-details-content2": {
    "children": [
      "drawer#searchMenuMobileMen",
      "flex-layout.row#linkShopWomen",
      "drawer#subcategory-1",
      //"drawer#subcategory-2",
      "drawer#subcategory-3",
      "drawer#subcategory-4",
      "drawer#subcategory-5",
      "drawer#subcategory-6",
      "drawer#subcategory-7",
      "info-card#menuMobile"
